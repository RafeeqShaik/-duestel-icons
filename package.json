{
  "name": "@duestel/icons",
  "version": "1.0.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/RafeeqShaik/-duestel-icons.git"
  },
  "scripts": {
    "fix-icon-names": "npx ts-node scripts/fix-icon-names.ts",
    "generate-icons": "npx ts-node scripts/pre-script.ts && npm run fix-icon-names && npx @svgr/cli --typescript --ext tsx --index-template ./scripts/svgr-index.template.js --icon --native --out-dir src/icons icons",
    "generate-index": "npx ts-node scripts/generate-index.ts",
    "generate-and-modify-icons": "npm run generate-icons && npx ts-node scripts/modify-icons.ts && npm run generate-index",
    "build": "npm run generate-and-modify-icons && tsup",
    "prepublishOnly": "npm run build",
    "publish": "npm publish --access public"
  },
  "keywords": [
    "icons",
    "svg",
    "react",
    "typescript",
    "react-components",
    "svg-icons",
    "customizable-icons",
    "icon-library",
    "web-development",
    "frontend"
  ],
  "author": "",
  "license": "ISC",
  "description": "A collection of customizable SVG icons, optimized for modern web development with TypeScript support.",
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-native-svg": "^15.10.1",
    "svgo": "^3.3.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@types/node": "^22.10.2"
  }
}
